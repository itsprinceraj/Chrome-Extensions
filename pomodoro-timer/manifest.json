{
  "manifest_version": 3,
  "name": "Pomo-Focus",
  "description": "A pomodoro timer for which helps you to complete your task with focus and increase your productivity.",
  "version": "1.1.0",
  "icons": {
    "16": "./icons/clock-9-16.png",
    "48": "./icons/clock-9-48.png",
    "128": "./icons/clock-9-128.png"
  },
  "background": {
    "service_worker": "./background/service-worker.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "contextMenus",
    "alarms",
    "notifications"
  ],
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./icons/clock-9-16.png"
  }
}
